    "typescript": "^4.0.3",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "webpack serve --env API_URL=http://localhost:8080 --config ./webpack.config.js --mode=development",
    "build": "webpack --config ./webpack.config.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "clean": "npm run lint && npm run prettier",
    "clean:fix": "npm run lint:fix && npm run prettier:fix",
    "lint": "eslint \"src/**/*.{js,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,ts,tsx}\"",
    "prettier": "prettier --check \"src/**/*.{js,ts,tsx}\"",
    "prettier:fix": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "help": "webpack --help"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix \"src/**/*.{js,ts,tsx}\""
    ],
    "*.{json,md,html}": [
      "prettier --write \"src/**/*.{js,ts,tsx}\""
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/eslint-parser": "^7.15.7",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@rushstack/eslint-patch": "^1.0.6",
    "@types/node": "^12.20.27",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "autoprefixer": "^10.3.6",
    "babel-loader": "^8.2.2",
    "confusing-browser-globals": "^1.0.10",
    "css-loader": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-testing-library": "^3.9.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "react-svg-loader": "^3.0.3",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.3.0",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "webpack": "^5.54.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.1"
  }
